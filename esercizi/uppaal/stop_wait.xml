<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
chan ack, send, send_ack, received;
clock time_elapsed, msg_time;
const int minTransmissionTime = 95;
const int maxTransmissionTime = 105;</declaration>
	<template>
		<name x="5" y="5">Sender</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="-433" y="-170">
			<name x="-443" y="-204">prepare</name>
		</location>
		<location id="id1" x="-144" y="-170">
			<name x="-154" y="-204">sending</name>
		</location>
		<location id="id2" x="-306" y="-297">
			<name x="-316" y="-331">wait</name>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id2"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-415" y="-250">ack?</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-288" y="-250">send!</label>
			<label kind="assignment" x="-238" y="-272">time_elapsed := 0</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
		</transition>
	</template>
	<template>
		<name>Receiver</name>
		<location id="id3" x="0" y="0">
			<name x="-10" y="-34">receive</name>
		</location>
		<location id="id4" x="127" y="-153">
			<name x="117" y="-187">processing</name>
		</location>
		<location id="id5" x="255" y="0">
			<name x="245" y="-34">acknowledge</name>
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id4"/>
			<target ref="id3"/>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id4"/>
			<label kind="synchronisation" x="195" y="-102">send_ack!</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="93" y="8">received?</label>
		</transition>
	</template>
	<template>
		<name>Channel</name>
		<location id="id6" x="-212" y="-229">
		</location>
		<location id="id7" x="-347" y="-118">
			<name x="-390" y="-93">to_sender</name>
			<label kind="invariant" x="-433" y="-59">msg_time &lt;= maxTransmissionTime</label>
		</location>
		<location id="id8" x="-76" y="-348">
			<name x="-86" y="-382">to_receiver</name>
		</location>
		<init ref="id6"/>
		<transition>
			<source ref="id7"/>
			<target ref="id6"/>
			<label kind="guard" x="-323" y="-110">msg_time &gt;= minTransmissionTime</label>
			<label kind="synchronisation" x="-306" y="-136">received!</label>
			<nail x="-228" y="-118"/>
		</transition>
		<transition>
			<source ref="id6"/>
			<target ref="id7"/>
			<label kind="synchronisation" x="-357" y="-255">send?</label>
			<label kind="assignment" x="-382" y="-272">msg_time := 0</label>
			<nail x="-331" y="-229"/>
		</transition>
		<transition>
			<source ref="id8"/>
			<target ref="id6"/>
			<label kind="guard" x="-297" y="-416">msg_time &gt;= minTransmissionTime</label>
			<label kind="synchronisation" x="-203" y="-390">ack!</label>
			<nail x="-195" y="-348"/>
		</transition>
		<transition>
			<source ref="id6"/>
			<target ref="id8"/>
			<label kind="synchronisation" x="-145" y="-212">send_ack?</label>
			<label kind="assignment" x="-161" y="-229">msg_time := 0</label>
			<nail x="-85" y="-229"/>
		</transition>
	</template>
	<system>sender = Sender();
receiver = Receiver();
channel = Channel();


system sender, receiver, channel;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
